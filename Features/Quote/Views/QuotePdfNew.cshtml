@model GidIndustrial.Gideon.WebApi.Models.Quote

@{
    string pageHeight = "";

    @if (Model.LineItems.Count < 8)
    { pageHeight = "min-height: 1702px;"; }
    else if (Model.LineItems.Count >= 8 && Model.LineItems.Count < 22)
    { pageHeight = "min-height: 3541px;"; }
    else
    { pageHeight = "min-height: 5387px;"; }
}

