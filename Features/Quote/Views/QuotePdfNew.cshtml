<body>
    <section>
        <div class="header row-center-between">
            <div>
                <img src="@ViewData["LogoUrlNew"]" alt="">
            </div>
            <div class="header-text">
                <p>@Model.GidLocationOption.MainAddress.Name</p>
                <p>@Model.GidLocationOption.MainAddress.Address1</p>
                @if (Model.GidLocationOption.MainAddress.Address2 != null)
                {
                    <p>@Model.GidLocationOption.MainAddress.Address2</p>
                }
                <p>@Model.GidLocationOption.MainAddress.City, @Model.GidLocationOption.MainAddress.State @Model.GidLocationOption.MainAddress.ZipPostalCode</p>
            </div>
        </div>
    </section>
    <section>
        <div class="page-content">
            <p class="sales-order">Quote #@Model.Id</p>
            <div class="row-ìnformation ">
                <div class="card2">
                    <div class="card-header">
                        <p>Information</p>
                    </div>
                    <div class="card2-content">
                        <div class="card-item row-center-between">
                            <p class="label-normal">Created</p>
                            <p class="label-normal">@Model.CreatedAt.Value.ToString("d")</p>
                        </div>
                        <div class="card-item row-center-between">
                            <p class="label-normal">Expires</p>
                            <p class="label-normal">@Convert.ToDateTime(Model.Expiration).ToString("d")</p>
                        </div>
                        @if (Model.ShippingMethod != null)
                        {
                            if (Model.ShippingMethod.Value != "FOB Richardson TX USA")
                            {
                                <div class="card-item row-center-between">
                                    <p class="label-normal">Shipping method</p>
                                    <p class="label-normal">@Model.ShippingMethod.Value</p>
                                </div>
                            }
                        }
                        @if (Model.PaymentTerm != null)
                        {
                            <div class="card-item row-center-between">
                                <p class="label-normal">Payment terms</p>
                                <p class="label-normal">@Model.PaymentTerm.Name</p>
                            </div>
                        }
                    </div>
                </div>
                <div class="card2">
                    <div class="card-header">
                        <p>Prepared for</p>
                    </div>
                    <div class="card2-content">
                        <div class="card-item row-center-between">
                            @* <p class="label-normal">Name</p> *@
                            <p class="label-normal">@Model.Contact.FirstName @Model.Contact.LastName</p>
                        </div>

                        @if (Model.Company != null && Model.Company.Name != null && Model.Company.Name != "")
                        {
                            <div class="card-item row-center-between">
                                @* <p class="label-normal">Company</p> *@
                                <p class="label-normal">@Model.Company.Name</p>
                            </div>
                        }

                        <div class="card-item row-center-between">
                            @* <p class="label-normal">Email</p> *@
                            <p class="label-normal">@Model.Email.ToLower()</p>
                        </div>
                        <div class="card-item row-center-between">
                            @* <p class="label-normal">Phone</p> *@
                            <p class="label-normal">@Model.PhoneNumberFormatted</p>
                        </div>

                    </div>
                </div>
                <div class="card2">
                    <div class="card-header">
                        <p>Prepared by</p>
                    </div>
                    <div class="card2-content">
                        <div class="card-item row-center-between">
                            @* <p class="label-normal">Name</p> *@
                            <p class="label-normal">@Model.SalesPerson.FirstName @Model.SalesPerson.LastName</p>
                        </div>
                        <div class="card-item row-center-between">
                           @*  <p class="label-normal">Email</p> *@
                            <p class="label-normal">@Model.SalesPerson.Email.ToLower()</p>
                        </div>
                        <div class="card-item row-center-between">
                            @* <p class="label-normal">Work Phone</p> *@
                            <p class="label-normal">w) @Model.SalesPerson.WorkPhone</p>
                        </div>
                        <div class="card-item row-center-between">
                            @* <p class="label-normal">Mobile Phone</p> *@
                            <p class="label-normal">m) @Model.SalesPerson.MobilePhone</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="line-items">
                <div class="card">
                    <div class="card-header">
                        <p>Line items</p>
                    </div>
                    <div class="card-content-table">

 @{
                            double grandTotalPrice = 0;
                            double subTotal = 0;
 }

                        <table class="table-items-list" style="overflow: visible !important;background: none !important;">
                            <thead style="display: table-header-group; page: auto; background: none !important;">
                            <tr>
                                <th id="Product">Product</th>
                                <th id="Manufacturer">Manufacturer</th>
                                <th id="Condition">Condition</th>
                                <th id="Lead-time">Lead-Time</th>
                                <th id="Warranty">Warranty</th>
                                <th id="Price">Cost</th>
                                <th id="Quantity">Qty</th>
                                <th id="Total">Total</th>
                            </tr>
                            </thead>


                            @foreach (var lineItem in Model.LineItems)
                                        {
                                            var productName = lineItem.Product != null ? lineItem.Product.PartNumber : lineItem.ProductName;
                                            if (lineItem.DisplayPartNumber != null && lineItem.DisplayPartNumber != "")
                                            {
                                                productName = lineItem.DisplayPartNumber;
                                            }
                                            var price = lineItem.Price == null ? 0 : lineItem.Price;
                                            var priceAfterDiscount = (double)price;
                                            if (lineItem.DiscountPercent != null && lineItem.DiscountPercent > 0)
                                            {
                                                priceAfterDiscount = ((double)price) * (100 - (double)lineItem.DiscountPercent) * .01;
                                            }
                                            double itemTotalPrice;
                                            if (lineItem.Quantity > 0)
                                            {
                                                itemTotalPrice = priceAfterDiscount * (double)lineItem.Quantity;
                                            }
                                            else
                                            {
                                                itemTotalPrice = priceAfterDiscount;
                                            }
                                            subTotal += itemTotalPrice;
                                            var description = lineItem.Description ?? "";
                                            if (description == "" && lineItem.Product != null && lineItem.Product.Description != null)
                                            {
                                                description = lineItem.Product.Description;
                                            }

                                <tbody>
                                <tr>
                                        <td id="Product">@productName</td>
                                        <td id="Manufacturer">@(lineItem.Product.Manufacturer != null ? lineItem.Product.Manufacturer.Name : "")</td>
                                        <td id="Condition">@(lineItem.Condition != null ? lineItem.Condition.Value : "")</td>
                                        <td id="Lead-time">@(lineItem.LeadTime != null ? lineItem.LeadTime.Value : "")</td>
                                        <td id="Warranty">@(lineItem.Warranty != null ? lineItem.Warranty.Value : "")</td>
                                        <td id="Price">@Model.CurrencyOption.Symbol@priceAfterDiscount.ToString("N2")</td>
                                        <td id="Quantity">@lineItem.Quantity</td>
                                        <td id="Total">@Model.CurrencyOption.Symbol@itemTotalPrice.ToString("N2")</td>
                                </tr>
                                <tr>
                                    <td colspan="8" id="Description">
                                        @description
                                    </td>
                                </tr>
                                </tbody>

                                            }



                            @{
                                grandTotalPrice = subTotal;
                                if (Model.SalesTax > 0)
                                {
                                    grandTotalPrice += (double)Model.SalesTax;
                                }
                                else
                                {
                                    Model.SalesTax = 0;
                                }
                                if (Model.WireTransferFee > 0)
                                {
                                    grandTotalPrice += (double)Model.WireTransferFee;
                                }
                                else
                                {
                                    Model.WireTransferFee = 0;
                                }
                                if (Model.CreditCardFee > 0)
                                {
                                    grandTotalPrice += (double)Model.CreditCardFee;
                                }
                                else
                                {
                                    Model.CreditCardFee = 0;
                                }
                                if (Model.ShippingAndHandlingFee > 0)
                                {
                                    grandTotalPrice += (double)Model.ShippingAndHandlingFee;
                                }
                                else
                                {
                                    Model.ShippingAndHandlingFee = 0;
                                }
                                if (Model.ExpediteFee > 0)
                                {
                                    grandTotalPrice += (double)Model.ExpediteFee;
                                }
                                else
                                {
                                    Model.ExpediteFee = 0;
                                }
                            }
                           
                        </table>




                        <div class="line-items-total">
                            <div class="line-items-total-detail">
                                <div class="line-items-total-detail-row row-center-between">
                                    <p class="label-normal">Subtotal</p>
